{
    "name": "mostafarde/storage-manager",
    "description": "Storage manager for laravel framework",
    "type": "metapackage",
    "authors": [
        {
            "name": "Mostafa Rostam Dokht Effati",
            "email": "mostafa.rostamdokht@gmail.com"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
            "MostafaRDE\\StorageManager\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MostafaRDE\\StorageManager\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "MostafaRDE\\StorageManager\\StorageManagerServiceProvider"
            ],
            "aliases": {
                "Alert": "MostafaRDE\\StorageManager\\Facades\\StorageManager"
            }
        }
    }
}
